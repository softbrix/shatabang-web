  },
  "keywords": [
    "pictures",
    "gallery",
    "shatabang",
    "softbrix",
    "opencv",
    "docker",
    "ember",
    "node"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "axios": "0.9.1",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "base85": "^2.1.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "connect-redis": "^3.0.2",
    "exif": "*",
    "exiftool": "0.0.3",
    "express": "4.13.3",
    "express-session": "^1.13.0",
    "faced": "^1.2.0",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "*",
