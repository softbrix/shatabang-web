    "multer": "1.1.0",
    "node-dir": "*",
    "node-sha1": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "passport": "^0.3.2",
    "passport-google-oauth20": "1.0.0",
    "passport-local": "1.0.0",
    "path": "*",
    "piexifjs": "^1.0.0",
    "progress": "1.1.8",
    "q": "^1.4.1",
    "redis": "^2.5.3",
    "sha1-file": "^1.0.0",
    "sha256": "0.2.0",
    "sharp": "^0.18.4",
